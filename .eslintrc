{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "rules": {
    "indent": [2, 2],
    "import/prefer-default-export": "off",
    "no-console":"off",
    "import/extensions":"off",
    "linebreak-style":"off",
    "no-param-reassign":"off"
  }
}
